{
    "name": "thirtybittech/autoreply",
    "type": "statamic-addon",
    "version": "dev-fix/navigation-click-error",
    "description": "AutoReply enables automatic email responses for submitted forms, ensuring timely engagement and consistent communication with users.",
    "authors": [
        {
            "name": "30Bit Tech",
            "email": "contact@30-bit.com",
            "homepage": "https://30-bit.com",
            "role": "Founder"
        }
    ],
    "autoload": {
        "psr-4": {
            "ThirtyBitTech\\Autoreply\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "edalzell/forma": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Autoreply",
            "description": "AutoReply enables automatic email responses for submitted forms, ensuring timely engagement and consistent communication with users."
        },
        "laravel": {
            "providers": [
                "ThirtyBitTech\\Autoreply\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
